{
  "name": "@ucd/genie",
  "version": "0.0.0",
  "description": "A CLI tool to generate data models from UCD (Unicode Character Database) files.",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.8.1",
  "license": "MIT",
  "homepage": "https://github.com/ucdjs/genie#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ucdjs/genie.git"
  },
  "bugs": "https://github.com/ucdjs/genie/issues",
  "keywords": [
    "unicode",
    "ucd",
    "ucdjs",
    "character",
    "database"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "genie": "bin/genie.mjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest --run",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "farver": "^0.4.2",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^4.18.1",
    "@types/node": "^22.10.9",
    "@types/yargs-parser": "^21.0.3",
    "eslint": "^9.25.0",
    "publint": "^0.3.12",
    "tsdown": "^0.9.1",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  }
}
